{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "max-len": ["error", 180, 2],
    "func-names": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": "warn"
  }
}